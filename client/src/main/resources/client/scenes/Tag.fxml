<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="tagHBox" alignment="CENTER" prefHeight="11.0" prefWidth="70.0" spacing="10.0" style="-fx-background-color: #CAE2F0;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.TagCtrl">

    <Label fx:id="tagLabel" alignment="CENTER" text="Tag" textFill="#2274a5" />
    <Button mnemonicParsing="false" prefHeight="19.0" prefWidth="14.0" style="-fx-border-radius: 50%; -fx-background-color: transparent; -fx-border-color: #a32222;" text="x" textFill="#a32222">
        <cursor>
            <Cursor fx:constant="HAND" />
        </cursor></Button>

    <HBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </HBox.margin>
</HBox>
